{
  "name": "@mock-fried/openapi-client",
  "version": "1.0.0",
  "description": "Generated TypeScript client for mock-fried testing (via openapi-generator)",
  "main": "./src/index.ts",
  "module": "./src/index.ts",
  "types": "./src/index.ts",
  "exports": {
    ".": "./src/index.ts",
    "./apis": "./src/apis/index.ts",
    "./models": "./src/models/index.ts",
    "./package.json": "./package.json"
  },
  "files": [
    "src",
    "openapi.yaml"
  ],
  "scripts": {
    "generate": "openapi-generator-cli generate -i openapi.yaml -g typescript-fetch -o src --additional-properties=supportsES6=true,typescriptThreePlus=true"
  },
  "license": "MIT"
}
