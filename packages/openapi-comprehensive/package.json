{
  "name": "@mock-fried/openapi-comprehensive",
  "version": "1.0.0",
  "description": "Comprehensive OpenAPI test package for mock-fried module",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./apis": {
      "types": "./dist/apis/index.d.ts",
      "import": "./dist/apis/index.js"
    },
    "./models": {
      "types": "./dist/models/index.d.ts",
      "import": "./dist/models/index.js"
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "clean": "rm -rf dist"
  },
  "devDependencies": {
    "typescript": "^5.3.0"
  },
  "keywords": [
    "openapi",
    "mock",
    "test",
    "comprehensive"
  ],
  "license": "MIT"
}
